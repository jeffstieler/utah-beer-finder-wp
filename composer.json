{
	"name": "jeffstieler/brewtah",
	"description": "Find the best beer available in Utah",
	"authors": [
		{
			"name": "Jeff Stieler",
			"email": "jeff.m.stieler@gmail.com"
		}
	],
	"type": "project",
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "wordpress",
				"type": "webroot",
				"version": "4.0",
				"dist": {
					"type": "zip",
					"url": "http://wordpress.org/wordpress-4.0.zip"
				},
				"require" : {
					"fancyguy/webroot-installer": "1.0.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "upstatement/jigsaw",
				"type": "wordpress-plugin",
				"version": "0.4.11",
				"source": {
					"url": "https://github.com/Upstatement/jigsaw.git",
					"type": "git",
					"reference": "5c3d9c4fbb095246500c63c2aa9c4cb09bbb8688"
				},
				"require" : {
					"composer/installers": "~1.0"
				}
			}
		},
		{
			"type": "composer",
			"url": "http://wpackagist.org"
		}
	],
	"config": {
		"vendor-dir": "wp-content/themes/brewtah/vendor"
	},
	"require": {
		"wordpress": "~4.0",
		"fancyguy/webroot-installer": "~1.0",
		"symfony/css-selector": "~2.6",
		"symfony/dom-crawler": "~2.5",
		"wpackagist-plugin/titan-framework": "~1.5",
		"upstatement/jigsaw": "~0.4",
		"voceconnect/objects-to-objects": "~1.2"
	},
	"require-dev" : {
		"wpackagist-plugin/developer" : "~1.2"
	},
	"extra": {
		"webroot-dir": "wp",
		"webroot-package": "wordpress",
		"installer-paths": {
			"wp-content/themes/brewtah/plugins/{$name}/": [
				"wpackagist-plugin/titan-framework",
				"upstatement/jigsaw"
			],
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	}
}	